    <div class="mb-5 pt-3">
        <a href="/dashboard/add/"><button id="add-post" class="button">Add New Blog Entry</button></a>
        
        <h2 class="title">Welcome to your dashboard!</h2>
{{#if entries.length}}
    </div>

    {{#each entries as |entry|}}
        {{> entryInfo entry}}
        
            <a href="/dashboard/entry/{{entry.id}}"><button class="button">Edit Entry</button></a>
        </div>
    {{/each}}

{{else}}
<h4 class="">No Posts Yet!</h4>

{{/if}}
